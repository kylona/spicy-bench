public class DoAll2OrigNo {

  int[][] a = new int[100][100];

  public static void main(String[] args) {
    forall (point[i] : [0:100-1]) {
      for (int j = 0; j < 100; j++) {
        a[i][j] = a[i][j]+1;
      }
    }
  }
}
