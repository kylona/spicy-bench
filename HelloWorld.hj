import java.util.*;

public class HelloWorld {

  static int count = 0;

  public static void foo(int i) {
    System.out.println("Hello(" + i +")");
    System.out.println(count);
  }

  public static void main(String[] args) {
    forall (point[i] : [0:10]) {
        int newCount = count + 1;
        foo(i);
        count = newCount;
      }

  }
